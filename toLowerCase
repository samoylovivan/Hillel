package ivan.samoylov;

public class Main {

    public static void main(String[] args) {
        String str = "#1AmCZYTREV";
        char[] chars = str.toCharArray();
        for (int i = 0; i < chars.length; i++){
            if (chars[i] >= 65 && chars[i] <= 90) chars[i] += 32;
            System.out.print(chars[i]);
        }
    }
}
